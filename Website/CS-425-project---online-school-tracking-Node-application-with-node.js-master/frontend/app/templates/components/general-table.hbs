      <tbody>
        {{#if isTable1}}
          <tr>
            <td class="col-lg-4">{{model.books.length}}</td>
            <td class="col-lg-4 red">{{model.amountFalse.length}}</td>
            <td class="col-lg-4 green">{{model.amountTrue.length}}</td>
          </tr>
        {{/if}}
        {{#if isTable2}}
          {{#each model.books as |book|}}
            <tr>
            {{#unless book.is_available}}
              <td class="col-lg-4">{{book.student.student_name}}</td>
              <td class="col-lg-4 blue">{{book.checkout_date}}</td>
              <td class="col-lg-4 red">{{book.due_date}}</td>
            {{/unless}}
            </tr>
          {{/each}}
        {{/if}}
        {{#if isTable3}}
          {{#each model.books as |book|}}
            <tr>
            {{#if book.is_due}}
              <td class="col-lg-4">{{book.student.student_name}}</td>
              <td class="col-lg-4"><span class="blue">{{book.student.parent.mother_name}}</span>&nbsp;|&nbsp;<span class="green">{{book.student.parent.father_name}}</span></td>
              <td class="col-lg-4 red">${{book.price}}</td>
            {{/if}}
            </tr>
          {{/each}}
        {{/if}}
        {{#if isAdmin}}
          {{#each model as |admin|}}
          <tr {{action 'editAdmin' admin.id}} class="highlight-hover">
            <!-- <td class="col-lg-2">{{admin.id}}</td> -->
            <td class="col-lg-5">{{admin.admin_name}}</td>
            <td class="col-lg-5">{{admin.admin_email}}</td>
            <td class="col-lg-2"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' admin.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isBook}}
          {{#each model as |book|}}
          <tr {{action 'editBook' book.id}} class="highlight-hover">
            <td class="col-lg-2">{{book.book_name}}</td>
            <td class="col-lg-2">{{book.isbn}}</td>
            <td class="col-lg-2">{{book.author}}</td>
            <td class="col-lg-1">${{book.price}}</td>
            <td class="col-lg-1">{{book.is_available}}</td>
            <td class="col-lg-2">{{book.checkout_date}}</td>
            <td class="col-lg-1">{{book.due_date}}</td>
            <!-- <td class="col-lg-1">{{book.student.student_name}}</td>
            <td class="col-lg-1">{{book.teacher.teacher_name}}</td> -->
            <td class="col-lg-1"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' book.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isBookCO}}
          {{#each model as |book|}}
            {{#unless book.is_available}}
              <tr {{action 'editBook' book.id}} class="highlight-hover">
                <td class="col-lg-2">{{book.book_name}}</td>
                <td class="col-lg-2">{{book.isbn}}</td>
                <td class="col-lg-2">{{book.author}}</td>
                <td class="col-lg-1">${{book.price}}</td>
                <td class="col-lg-1">{{book.is_available}}</td>
                <td class="col-lg-2">{{book.checkout_date}}</td>
                <td class="col-lg-1">{{book.due_date}}</td>
                <!-- <td class="col-lg-1">{{book.student.student_name}}</td>
                <td class="col-lg-1">{{book.teacher.teacher_name}}</td> -->
                <td class="col-lg-1"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' book.id bubbles=false}}>Delete</button></td>
              </tr>
            {{/unless}}
          {{/each}}
        {{/if}}
        {{#if isStudent}}
          {{#each model as |student|}}
          <tr {{action 'editStudent' student.id}} class="highlight-hover">
            <td class="col-lg-1">{{student.student_ID}}</td>
            <td class="col-lg-2">{{student.student_name}}</td>
            <td class="col-lg-2">{{student.student_email}}</td>
            <td class="col-lg-2">{{student.student_address}}</td>
            <td class="col-lg-1">{{student.student_phone}}</td>
            <td class="col-lg-1">{{student.parent.mother_name}}</td>
            <td class="col-lg-1">{{student.department.dept_name}}</td>
            <td class="col-lg-1">{{student.advisor.advisor_name}}</td>
            <td class="col-lg-1"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' student.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isParent}}
          {{#each model as |parent|}}
          <tr {{action 'editParent' parent.id}} class="highlight-hover">
            <!-- <td class="col-lg-2">{{parent.id}}</td> -->
            <td class="col-lg-3">{{parent.mother_name}}</td>
            <td class="col-lg-3">{{parent.father_name}}</td>
            <!-- <td class="col-lg-4">{{parent.parent_phone}}</td> -->
            <td class="col-lg-3 wrap">
              {{#each parent.students as |student|}}
                {{student.student_name}}
                |&nbsp;
              {{/each}}
            </td>
            <td class="col-lg-3"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' parent.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isTeacher}}
          {{#each model as |teacher|}}
          <tr {{action "editTeacher" teacher.id}} class="highlight-hover">
            <td class="col-lg-4">{{teacher.teacher_name}}</td>
            <td class="col-lg-2">{{teacher.teacher_email}}</td>
            <td class="col-lg-2 wrap">
              {{#each teacher.classes as |class|}}
                {{class.class_name}}
                |&nbsp;
              {{/each}}
            </td>
            <td class="col-lg-2">{{teacher.department.dept_name}}</td>
            <td class="col-lg-2"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' teacher.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isClass}}
