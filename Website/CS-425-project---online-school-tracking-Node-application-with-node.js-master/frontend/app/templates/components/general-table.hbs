            <!-- <td class="col-lg-2">{{user.id}}</td> -->
            <td class="col-lg-4">{{user.user_name}}</td>
            <td class="col-lg-4">{{user.user_email}}</td>
            <td class="col-lg-4">{{user.department.dept_name}}</td>
            <td class="col-lg-2"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' user.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
