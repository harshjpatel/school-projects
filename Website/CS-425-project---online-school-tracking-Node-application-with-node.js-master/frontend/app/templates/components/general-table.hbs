            {{/each}}
            {{#each department.advisors as |advisor|}}
            <td class="col-lg-2">{{advisor.advisor_name}}</td>
            {{/each}}
            {{#each department.classes as |class|}}
            <td class="col-lg-2">{{class.class_name}}</td>
            {{/each}} -->
            <td class="col-lg-2"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' department.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isAdvisor}}
