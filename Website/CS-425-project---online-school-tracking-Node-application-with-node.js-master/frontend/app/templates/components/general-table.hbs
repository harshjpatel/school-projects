            <!-- <td class="col-lg-2">{{advisor.id}}</td> -->
            <td class="col-lg-4">{{advisor.advisor_name}}</td>
            <td class="col-lg-4">{{advisor.advisor_email}}</td>
            <td class="col-lg-2">{{advisor.department.dept_name}}</td>
            <!-- {{#each advisor.students as |student|}}
