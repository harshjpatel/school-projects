            {{/each}} -->
            <td class="col-lg-2"><button type="button" name="button" class="delete-btn" {{action 'confirmDelete' advisor.id bubbles=false}}>Delete</button></td>
          </tr>
          {{/each}}
        {{/if}}
        {{#if isUser}}
